{
  "expo": {
    "name": "Portfolio MAX",
    "slug": "portfolio-max",
    "description": "A simple portfolio app, ft. React Native.",
    "privacy": "public",
    "version": "1.0.0",
    "platforms": ["android"],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#b6d0e2"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#b6d0e2"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
